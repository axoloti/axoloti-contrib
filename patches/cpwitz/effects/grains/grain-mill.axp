   <patchobj type="patch/object" uuid="0836850a-cc41-415b-8338-05548bbb3cb5" name="buffer" x="224" y="560">
      <params>
         <bool32.tgl name="freeze" value="0"/>
      </params>
      <attribs>
         <objref attributeName="table" obj="buffertable"/>
