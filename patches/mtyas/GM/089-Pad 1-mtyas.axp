if(lastPhase > phase) {
	wt1 = new_wt1; 
	wt2 = new_wt2; 
	wt1m = new_wt1m; 
	wt2m = new_wt2m; 
	mwt = new_mwt; 
	cwt = new_cwt; 
}
lastPhase = phase;

//div number of entries
phasor = phasor >> attr_table.WAVESPOW;


//read interp - wt1
int32_t swt1;
{
uint32_t asat = __USAT(phasor + wt1,27);
int index = asat >> (27-attr_table.LENGTHPOW);
