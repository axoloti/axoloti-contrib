		uint32_t d  = w - (w & WAVESMASK);
		uint32_t dp  = d << attr_table.WAVESPOW ;
		new_mwt = ((128<<20) - dp);
		new_cwt = dp;
	}
	uint32_t wsi = __USAT(new_wt2,27) >> (27-attr_table.LENGTHPOW);
	new_wt2m = (wsi + (1 << attr_table.SIZEPOW) ) -1 ;
