   <patchobj type="patch/object" uuid="e0bfa327-cdca-4650-88d1-69557d8fc279" name="object_1" x="518" y="266">
      <params/>
      <attribs>
         <objref attributeName="slicesbuffer" obj="slicesbuf"/>
         <objref attributeName="slicesindex" obj="slicesind"/>
      </attribs>
      <object id="patch/object" uuid="e0bfa327-cdca-4650-88d1-69557d8fc279">
         <inlets>
            <int32 name="nslice" description=""/>
            <frac32 name="pitch" description=""/>
            <frac32 name="loopfreq" description=""/>
            <frac32.positive name="chunksize"/>
         </inlets>
         <outlets>
            <frac32buffer name="o"/>
         </outlets>
         <displays/>
         <params/>
         <attribs>
            <objref name="slicesbuffer"/>
            <objref name="slicesindex"/>
         </attribs>
         <includes/>
         <code.declaration><![CDATA[int64_t pos1;
int64_t pos2;
int64_t pos3;
uint32_t beg;
uint32_t end;
uint32_t rfl; // repetition frequency of the loop
uint32_t fch; // chunk frequency
uint32_t f;
uint32_t nslice;]]></code.declaration>
         <code.init><![CDATA[pos1 = 0;
pos2 = 0;
pos3 = 0;
beg = 0;
end = 0;
rfl = 0;
f = 0;
nslice = 0xFFFF;]]></code.init>
         <code.krate><![CDATA[if(inlet_nslice < attr_slicesindex.array[0] && inlet_nslice != nslice) {
   nslice = inlet_nslice; 
   if(inlet_nslice == 0) {
      beg = 0;
      end = attr_slicesindex.array[1];
   } else {
      beg = (uint64_t)attr_slicesindex.array[inlet_nslice];
      end = attr_slicesindex.array[inlet_nslice+1];
   }
   pos1 = ((uint64_t)beg)<<32;
}

MTOFEXTENDED(inlet_loopfreq + 80179668,rfl); // repetition frequency of the loop
MTOFEXTENDED(inlet_pitch-inlet_loopfreq + 80179668,fch);]]></code.krate>
         <code.srate><![CDATA[uint32_t o = ___SMMUL(attr_slicesbuffer.array[pos3>>32]<<attr_slicesbuffer.GAIN,INT32_MAX-(((uint32_t)pos3)>>1));
outlet_o = ___SMMLA(attr_slicesbuffer.array[(pos3>>32)+1]<<attr_slicesbuffer.GAIN,(((uint32_t)pos3)>>1),o);

pos1 += ((uint64_t)rfl)<<4;
if((pos1>>32) > end) pos1 = ((uint64_t)beg)<<32;

pos2 += ((uint64_t)fch)<<4;
if((pos2>>32) > (4294944*(inlet_chunksize>>20))) pos2 = 0;

pos3 = pos1+pos2;
if((pos3>>32) > end) pos3 = end;]]></code.srate>
      </object>
   </patchobj>
