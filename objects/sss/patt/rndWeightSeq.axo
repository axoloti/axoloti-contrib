PExParameterChange(&parent->PExch[PARAM_INDEX_attr_legal_name_b13],(((uint32_t)((GenerateRandomNumber())))>>5),0xFFFD);
PExParameterChange(&parent->PExch[PARAM_INDEX_attr_legal_name_b14],(((uint32_t)((GenerateRandomNumber())))>>5),0xFFFD);
PExParameterChange(&parent->PExch[PARAM_INDEX_attr_legal_name_b15],(((uint32_t)((GenerateRandomNumber())))>>5),0xFFFD);
