         <int32 name="length"/>
         <int32 name="maxOct"/>
         <int32 name="minOct"/>
         <int32 name="pattern"/>
         <int32 name="chancePatt"/>
         <bool32 name="save"/>
         <bool32 name="load"/>
         <charptr32 name="filename"/>
      </inlets>
      <outlets>
         <frac32 name="random"/>
         <frac32 name="repeating"/>
      </outlets>
      <displays/>
      <params>
         <frac32.u.mapvsl name="C" noLabel="true">
