         </frac32.u.mapvsl>
         <frac32.u.mapvsl name="A" noLabel="true">
            <MinValue v="0.0"/>
            <MaxValue v="64.0"/>
         </frac32.u.mapvsl>
         <frac32.u.mapvsl name="ais" noLabel="true">
            <MinValue v="0.0"/>
